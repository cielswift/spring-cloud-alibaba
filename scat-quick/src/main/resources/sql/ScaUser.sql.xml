        xsi:schemaLocation="http://www.sagframe.com/schema/sqltoy http://www.sagframe.com/schema/sqltoy/sqltoy.xsd">
    <!-- 第一个演示sql -->
    <sql id="sqltoy_query_staffInfo">
        <value>
            <![CDATA[
            select * from sca_user t
            where #[t.username like :staffName]
                  #[and t.id = :status]
                ]]>
        </value>
    </sql>
